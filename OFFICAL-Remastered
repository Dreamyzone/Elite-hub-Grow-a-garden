    Callback = function(state)
        autoBuyGearEnabled = state
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local BuyGearStock = ReplicatedStorage.GameEvents:WaitForChild("BuyGearStock")

        if state then
            autoBuyGearConnection = task.spawn(function()
                while autoBuyGearEnabled do
                    pcall(function()
                        BuyGearStock:FireServer("Levelup Lollipop")
                    end)
                    task.wait(1)
                end
            end)
        else
            autoBuyGearEnabled = false
        end
    end
})

Tabs.Settings:Button({
    Title = "Anti-Lag",
    Desc = "Removes textures/particles to reduce lag",
    Callback = function()
        for _, v in pairs(workspace:GetDescendants()) do
            if v:IsA("ParticleEmitter") or v:IsA("Trail") then
                v.Enabled = false
            elseif v:IsA("Explosion") then
                v:Destroy()
            elseif v:IsA("Decal") or v:IsA("Texture") then
                v:Destroy()
            elseif v:IsA("MeshPart") and v.Material ~= Enum.Material.SmoothPlastic then
                v.Material = Enum.Material.SmoothPlastic
            elseif v:IsA("BasePart") then
                v.Reflectance = 0
            end
        end

        -- Lower lighting settings
        local Lighting = game:GetService("Lighting")
        Lighting.GlobalShadows = false
        Lighting.FogEnd = 9e9
        Lighting.Brightness = 0

        print("Anti-Lag Applied!")
    end
})
