local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))();
local Window = WindUI:CreateWindow({Title="Elite Hub (Grow a Garden) | Remastered",Icon="rbxassetid://129260712070622",IconThemed=true,Author="Dreamyzone",Folder="Elitehub_GrowAGarden",Size=UDim2.fromOffset(580, 460),Transparent=true,Theme="Dark",KeySystem={Key={"Elite_GAG1473890","HugoSpecialkey"},Note="Complete the key to unlock Grow a Garden features.",URL="https://discord.gg/9V3ZSzdV4V",SaveKey=true}});
local Tabs = {Main=Window:Tab({Title="Main",Icon="leaf",Desc="Grow a Garden Features"}),AutoBuySeeds=Window:Tab({Title="Auto Buy Seeds",Icon="shopping-cart",Desc="Auto seed purchases"}),AutoBuyGears=Window:Tab({Title="Auto Buy Gears",Icon="wrench",Desc="Auto Gear purchases"}),Settings=Window:Tab({Title="Settings",Icon="cog",Desc="Client Tweaks"})};
Tabs.Main:Button({Title="Sell Inventory",Desc="Sells your entire inventory instantly",Callback=function()
	local ReplicatedStorage = game:GetService("ReplicatedStorage");
	local Sell_Inventory = ReplicatedStorage.GameEvents:WaitForChild("Sell_Inventory");
	pcall(function()
		Sell_Inventory:FireServer();
	end);
end});
local infJumpEnabled = false;
local UIS = game:GetService("UserInputService");
local Player = game:GetService("Players").LocalPlayer;
Tabs.Main:Toggle({Title="Infinite Jump",Desc="Lets you jump infinitely",Default=false,Callback=function(state)
	infJumpEnabled = state;
end});
UIS.JumpRequest:Connect(function()
	if (infJumpEnabled and Player.Character and Player.Character:FindFirstChildOfClass("Humanoid")) then
		Player.Character:FindFirstChildOfClass("Humanoid"):ChangeState("Jumping");
	end
end);
local Player = game:GetService("Players").LocalPlayer;
local Humanoid = Player.Character and Player.Character:FindFirstChildOfClass("Humanoid");
Player.CharacterAdded:Connect(function(char)
	Humanoid = char:WaitForChild("Humanoid");
end);
Tabs.Main:Slider({Title="WalkSpeed",Desc="Adjust your walking speed",Default=16,Min=16,Max=200,Callback=function(value)
	if Humanoid then
		Humanoid.WalkSpeed = value;
	end
end});
Tabs.Main:Paragraph({Title="More shit coming soon! until then please use the other features",Content="Elite Hub for Grow a Garden. Auto-buy, farming tools & more coming soon."});
local autoBuyEnabled = false;
local autoBuyConnection;
Tabs.AutoBuySeeds:Toggle({Title="Auto Buy Carrot Seed",Desc="Buys Carrot Seeds every 1 second",Default=false,Callback=function(state)
	local FlatIdent_66556 = 0;
	local ReplicatedStorage;
	local BuySeedStock;
	while true do
		if (FlatIdent_66556 == 0) then
			autoBuyEnabled = state;
			ReplicatedStorage = game:GetService("ReplicatedStorage");
			FlatIdent_66556 = 1;
		end
		if (FlatIdent_66556 == 1) then
			BuySeedStock = ReplicatedStorage.GameEvents:WaitForChild("BuySeedStock");
			if state then
				autoBuyConnection = task.spawn(function()
					while autoBuyEnabled do
						pcall(function()
							BuySeedStock:FireServer("Tier 1", "Carrot");
						end);
						task.wait(1);
					end
				end);
			else
				autoBuyEnabled = false;
			end
			break;
		end
	end
end});
